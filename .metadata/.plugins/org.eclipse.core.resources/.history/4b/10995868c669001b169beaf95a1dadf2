package strings;
import java.util.Scanner;
public class StringReversal {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s = sc.next();
		
//		s = s.trim();
        String[] splitted = s.split(" ");
        for(String word: splitted)
        {
            System.out.print(word + " ");
        }
	}

}
