package whileLoops;
import java.util.Scanner;
public class PalindromeNumber {

	public static void main(String[] args) {
		// Check if the inputed number is Palindrome or not?
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int temp = n;
		int reverse = temp%10;
		
		while(temp>0)
		{
//			int m = (temp%10);
			temp/=10;
			reverse = ((reverse*10) + temp%10);
			
			
			
		}

	}

}
